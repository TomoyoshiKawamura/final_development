<p id="notice"><%= notice %></p>

<h1>Personal Informations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Post code</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Phone number</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personal_informations.each do |personal_information| %>
      <tr>
        <td><%= personal_information.name %></td>
        <td><%= personal_information.post_code %></td>
        <td><%= personal_information.address1 %></td>
        <td><%= personal_information.address2 %></td>
        <td><%= personal_information.phone_number %></td>
        <td><%= personal_information.user_id %></td>
        <td><%= link_to 'Show', personal_information %></td>
        <td><%= link_to 'Edit', edit_personal_information_path(personal_information) %></td>
        <td><%= link_to 'Destroy', personal_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personal Information', new_personal_information_path %>
